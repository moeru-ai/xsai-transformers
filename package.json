{
  "name": "@xsai-transformers/root",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "repository": "github:moeru-ai/xsai-transformers",
  "scripts": {
    "dev": "pnpm run packages:build && pnpm -r -F=\"./playground\" run dev",
    "build": "pnpm -r -F=\"./playground\" run build",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "nolyfill": "pnpm dlx nolyfill",
    "packages:build": "pnpm -rF=\"./packages/*\" run build",
    "postinstall": "npx simple-git-hooks && pnpm packages:build",
    "sizecheck": "npx vite-bundle-visualizer",
    "test": "vitest --coverage",
    "test:run": "vitest run",
    "typecheck": "pnpm -rF=\"./packages/*\" -F run --parallel typecheck",
    "up": "taze major -I"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/ni": "^24.3.0",
    "@cspell/dict-ru_ru": "^2.2.4",
    "@types/node": "^22.14.0",
    "@unocss/eslint-config": "^66.1.0-beta.10",
    "@unocss/eslint-plugin": "^66.1.0-beta.10",
    "@vitest/coverage-v8": "3.0.5",
    "bumpp": "^10.1.0",
    "changelogithub": "^13.13.0",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.5.0",
    "rollup": "^4.39.0",
    "simple-git-hooks": "^2.12.1",
    "taze": "^19.0.4",
    "typescript": "~5.8.3",
    "unbuild": "3.0.0-rc.11",
    "unocss": "^66.1.0-beta.10",
    "vite": "^6.2.5",
    "vite-plugin-inspect": "^11.0.0",
    "vitest": "^3.1.1"
  },
  "workspaces": [
    "packages/**",
    "playground"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "onnxruntime-node",
      "sharp"
    ],
    "overrides": {
      "array-flatten": "npm:@nolyfill/array-flatten@^1.0.44",
      "axios": "npm:feaxios@^0.0.23",
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
      "isarray": "npm:@nolyfill/isarray@^1.0.44",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1.0.44",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1.0.44",
      "side-channel": "npm:@nolyfill/side-channel@^1.0.44",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1.0.44"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
