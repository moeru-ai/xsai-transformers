{
  "name": "xsai-transformers",
  "type": "module",
  "version": "0.0.3",
  "description": "Transformers.js wrapper makes it easy to use with xsai",
  "author": {
    "name": "Moeru AI",
    "email": "neko@moeru.ai",
    "url": "https://github.com/moeru-ai"
  },
  "license": "MIT",
  "homepage": "https://github.com/moeru-ai/xsai-transformers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moeru-ai/xsai-transformers.git",
    "directory": "packages-top/xsai-transformers"
  },
  "bugs": "https://github.com/moeru-ai/xsai-transformers/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./embed/worker": {
      "types": "./dist/embed/worker.d.ts",
      "import": "./dist/embed/worker.mjs"
    },
    "./transcription/worker": {
      "types": "./dist/transcription/worker.d.ts",
      "import": "./dist/transcription/worker.mjs"
    },
    "./vad/worker": {
      "types": "./dist/vad/worker.d.ts",
      "import": "./dist/vad/worker.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "stub": "unbuild --stub",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "web-worker": "^1.5.0"
  },
  "peerDependenciesMeta": {
    "web-worker": {
      "optional": true
    }
  },
  "dependencies": {
    "@huggingface/transformers": "^3.5.0",
    "@xsai-ext/shared-providers": "catalog:",
    "@xsai-transformers/embed": "workspace:^",
    "@xsai-transformers/shared": "workspace:^",
    "@xsai-transformers/transcription": "workspace:^",
    "@xsai-transformers/vad": "workspace:^",
    "@xsai/embed": "catalog:",
    "@xsai/shared": "catalog:",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@webgpu/types": "^0.1.60"
  }
}
