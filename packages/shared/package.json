{
  "name": "@xsai-transformers/shared",
  "type": "module",
  "version": "0.0.3",
  "description": "Transformers.js wrapper makes it easy to use with xsai",
  "author": {
    "name": "Moeru AI",
    "email": "neko@moeru.ai",
    "url": "https://github.com/moeru-ai"
  },
  "license": "MIT",
  "homepage": "https://github.com/moeru-ai/xsai-transformers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moeru-ai/xsai-transformers.git",
    "directory": "packages/shared"
  },
  "bugs": "https://github.com/moeru-ai/xsai-transformers/issues",
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./base64": {
        "types": "./dist/base64/index.d.ts",
        "default": "./dist/base64/index.js"
      },
      "./types": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./base64": "./src/base64/index.ts",
    "./types": "./src/types/index.ts"
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "pkgroll",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.5.0",
    "@xsai-ext/shared-providers": "catalog:",
    "@xsai/shared": "catalog:",
    "defu": "^6.1.4",
    "onnxruntime-common": "^1.21.0"
  },
  "devDependencies": {
    "@webgpu/types": "^0.1.60"
  }
}
